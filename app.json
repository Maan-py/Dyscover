{
  "expo": {
    "name": "Dyscover",
    "slug": "Dyscover",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo-Dyscover.png",
    "scheme": "dyscover",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo-Dyscover.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.Ambatuwin.Dyscover"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo-Dyscover.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo-Dyscover.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Montserrat.ttf", "./assets/fonts/Montserrat-Italic.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "995bd08c-4e4a-483d-b336-afbbb26d349c"
      }
    }
  }
}
